{
  "name": "ljas-webpack",
  "version": "0.0.7",
  "description": "webpack configuration parts for webpack config compositions",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "format": "npm run format:check",
    "format:check": "prettier --check dist/**/*.js test/**/*.js *.md",
    "format:write": "prettier --write dist/**/*.js test/**/*.js *.md",
    "lint": "eslint dist/**/*.js test/**/*.js",
    "test": "webpack --config test/webpack.config.js",
    "test:dev": "webpack serve --config test/webpack.config.js --open",
    "ts-test": "webpack --config test/ts.config.js",
    "ts-test:dev": "webpack serve --config test/ts.config.js --open"
  },
  "devDependencies": {
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlean/ljas-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/mattlean/ljas-webpack/issues"
  },
  "homepage": "https://github.com/mattlean/ljas-webpack#readme",
  "author": "Matthew Lean <matt@mattlean.com>",
  "keywords": [
    "ljas-webpack",
    "lean-js-app-starter",
    "lean-start",
    "ljas",
    "lean",
    "javascript",
    "starter",
    "project",
    "boilerplate",
    "js",
    "application",
    "app",
    "webpack",
    "configuration",
    "config"
  ]
}
